<module xmlns="urn:jboss:module:1.1" name="cz.muni.exceptions">
    <resources>
        <resource-root path="exceptions-subsystem.jar"/>
    </resources>

    <dependencies>
        <system export="true">
            <paths>
                <path name="META-INF" />
            </paths>
            <exports>
                <include-set>
                    <path name="META-INF/persistence.xml" />
                </include-set>
            </exports>
        </system>

        <module name="javax.api"/>
        <module name="org.jboss.staxmapper"/>
        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.logging"/>
        <module name="org.jboss.vfs"/>

        <module name="org.javassist" />
        <module name="org.jboss.as.jpa.hibernate" slot="4"/>
        <module name="org.jboss.as.jpa"/>        
        <module name="org.jboss.as.transactions"/>
        
        <module name="javax.persistence.api" />
        <module name="org.hibernate" />
        <module name="javax.transaction.api"/>

        <module name="com.google.guava" />
        <module name="com.sun.tools" />
    </dependencies>
</module>
